<table class="table">
  <tr>
    <th>Trade id</th>
    <th>Created at</th>
    <th>Actions</th>
  </tr>
  <% @orders.each do |order| %>
    <tr>
      <td><%= order.trade_id %></td>
      <td><%= order.created_at %></td>
      <td><%= link_to "Show", order_path(order) %></td>
    </tr>
  <% end %>
</table>

<%= link_to "New Order", new_order_path %>

