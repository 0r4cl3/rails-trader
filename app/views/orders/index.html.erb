<table class="table">
  <tr>
    <th>Trade id</th>
    <th>Created at</th>
    <th>Instrument</th>
    <th>Side</th>
    <th>Units</th>
    <th>Actions</th>
  </tr>
  <% @orders.each do |order| %>
    <tr>
      <td><%= order.trade_id %></td>
      <td><%= order.created_at %></td>
      <td><%= order.instrument %></td>
      <td><%= order.side %></td>
      <td><%= order.units %></td>
      <td><%= link_to "Show", order_path(order) %> | <%= link_to 'Destroy', order_path(order), method: :delete, data: {confirm: "Are you sure?"} %></td>
    </tr>
  <% end %>
</table>

<%= link_to "New Order", new_order_path %>

